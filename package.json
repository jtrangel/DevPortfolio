{
	"name": "myapp",
	"version": "0.0.1",
	"private": true,
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"deploy": "npm run add-no-jekyll && node ./gh-pages.js",
		"gh-pages": "npm run build && npx gh-pages -d build",
		"add-no-jekyll": "echo $null > ./build/.nojekyll",
		"test": "playwright test",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test:unit": "vitest",
		"lint": "eslint ."
	},
	"devDependencies": {
		"@sveltejs/adapter-static": "^1.0.0-next.50",
		"tailwindcss": "^3.2.6",
		"@sveltejs/kit": "^1.22.1",
		"@types/cookie": "^0.5.1",
		"@typescript-eslint/eslint-plugin": "^5.61.0",
		"@typescript-eslint/parser": "^5.61.0",
		"eslint": "^8.44.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte": "^2.32.2",
		"prettier": "^2.0.0",
		"prettier-plugin-svelte": "^2.10.1",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.5",
		"tslib": "^2.6.0",
		"typescript": "^5.1.6",
		"vite": "^4.4.2",
		"sass": "^1.63.6",
		"svelte-icons": "^2.1.0",
		"autoprefixer": "^10.4.13",
		"gh-pages": "^5.0.0"
	},
	"type": "module"
}